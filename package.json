{
  "name": "LynxReact",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "simpletest": "rm -rf ./node_modules/jest-cli/.haste_cache && jest",
    "test": "rm -rf ./node_modules/jest-cli/.haste_cache && jest --verbose",
    "coverage": "rm -rf ./node_modules/jest-cli/.haste_cache && jest --verbose --coverage",
    "test:watch": "npm test -- --watch",
    "test-chrome": "node-debug --preload false --nodejs --harmony jest --runInBand",
    "start": "node node_modules/react-native/local-cli/cli.js start"
  },
  "dependencies": {
    "lodash": "^4.5.0",
    "path": "^0.12.7",
    "react-native": "^0.20.0",
    "react-native-animatable": "^0.5.0",
    "react-native-blur": "^0.7.10",
    "react-native-button": "^1.4.2",
    "react-native-code-push": "^1.7.3-beta",
    "react-native-fs": "^1.2.0",
    "react-native-image-picker": "^0.14.3",
    "react-native-maps": "^0.3.0",
    "react-native-orientation": "^1.14.0",
    "react-native-overlay": "^0.4.1",
    "react-native-router-flux": "^2.2.5",
    "react-native-sglistview": "^0.1.3",
    "react-native-spinkit": "0.0.7",
    "react-native-sqlite-storage": "^2.1.3",
    "react-native-tabs": "^0.1.10",
    "react-native-vector-icons": "^1.2.0",
    "react-native-video": "^0.7.1",
    "react-redux": "^3.1.2",
    "redux": "^3.3.1",
    "redux-logger": "^2.5.2",
    "redux-thunk": "^1.0.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-preset-react-native": "^1.4.0",
    "isomorphic-fetch": "2.2.1",
    "jest-cli": "0.8.2",
    "nock": "5.5.0",
    "react": "0.14.7",
    "react-addons-test-utils": "0.14.7"
  },
  "jest": {
    "scriptPreprocessor": "jestSupport/scriptPreprocess.js",
    "testPathDirs": [
      "app/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testFileExtensions": [
      "es6",
      "spec.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "__mocks__",
      "lodash",
      "bluebird",
      "isomorphic-fetch",
      "node-fetch",
      "encoding",
      "whatwg-fetch",
      "nock",
      "debug",
      "redux",
      "redux-thunk",
      "react-addons-test-utils",
      "react",
      "fbjs"
    ]
  }
}
